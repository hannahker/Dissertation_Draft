@article{ahmouda_analyzing_2018,
	title = {Analyzing the effect of earthquakes on {OpenStreetMap} contribution patterns and tweeting activities},
	volume = {21},
	issn = {1009-5020},
	url = {https://doi.org/10.1080/10095020.2018.1498666},
	doi = {10.1080/10095020.2018.1498666},
	abstract = {Natural disasters, such as wildfires, earthquakes, landslides, or floods, lead to an increase in topical information shared on social media and in increased mapping activities in volunteered geographic information (VGI) platforms. Using earthquakes in Nepal and Central Italy as case studies, this research analyzes the effects of natural disasters on short-term (weeks) and longer-term (half year) changes in OpenStreetMap (OSM) mapping behavior and tweet activities in the affected regions. An increase of activities in OSM during the events can be partially attributed to those focused OSM mapping campaigns, for example, through the Humanitarian OSM Team (HOT). Using source tags in OSM change-sets, it was found that only a small portion of external mappers actually travels to the affected regions, whereas the majority of external mappers relies on desktop mapping instead. Furthermore, the study analyzes the spatio-temporal sequence of posted tweets together with keyword filters to identify a subset of users who most likely traveled to the affected regions for support and rescue operations. It also explores where, geographically, earthquake information spreads within social networks.},
	number = {3},
	urldate = {2020-06-05},
	journal = {Geo-spatial Information Science},
	author = {Ahmouda, Ahmed and Hochmair, Hartwig H. and Cvetojevic, Sreten},
	month = jul,
	year = {2018},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/10095020.2018.1498666},
	keywords = {OpenStreetMap (OSM), Twitter, Volunteered geographic information (VGI), human dynamics, natural disaster},
	pages = {195--212}
