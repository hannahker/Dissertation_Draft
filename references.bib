@article{yang_temporal_2016,
	title = {Temporal {Analysis} on {Contribution} {Inequality} in {OpenStreetMap}: {A} {Comparative} {Study} for {Four} {Countries}},
	volume = {5},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	shorttitle = {Temporal {Analysis} on {Contribution} {Inequality} in {OpenStreetMap}},
	url = {https://www.mdpi.com/2220-9964/5/1/5},
	doi = {10.3390/ijgi5010005},
	abstract = {Contribution inequality widely exists in OpenStreetMap (OSM), which means that most data come from a minority of the contributors, while the majority only accounts for a small percentage of data. This phenomenon is of great importance to understanding from where the data come and how the project evolves. The investigation in this paper is dedicated to answering the following questions: How does contribution inequality change over time in OSM? Which group of contributors plays a more important role in influencing trends in contribution inequality: the “vocal minority” or the “silent majority”? To answer the first question, we provide overall measurements for contribution inequality using the Lorenz curve and the Gini coefficient. To answer the second question, we use quantile-based classifying strategy to analyze structural changes in the community, and use the Mann-Whitney-Wilcoxon test to analyze productivity changes. Our case study shows that in countries without significant imports, contributions become more unequal over time. This trend is consistent with the rapid expansion of the silent majority, even though other classes of contributors also grow at a slower pace. On the other hand, contribution inequality fluctuates a lot in countries with huge imports, and agrees well with the productivity changes in the vocal minority.},
	language = {en},
	number = {1},
	urldate = {2020-02-27},
	journal = {ISPRS International Journal of Geo-Information},
	author = {Yang, Anran and Fan, Hongchao and Jing, Ning and Sun, Yeran and Zipf, Alexander},
	month = jan,
	year = {2016},
	keywords = {OpenStreetMap, inequality, volunteered geographical information},
	pages = {5}
}

