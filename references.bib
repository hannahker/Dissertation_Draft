	keywords = {Buildings, Collaboration, Computers, Context, Disaster management, Earthquakes, Nepal earthquake, OpenStreetMap project, Roads, cartography, collaborative mapping, disaster management, disaster relief, disaster-affected areas, disasters, earthquakes, emergency management, relief efforts},
